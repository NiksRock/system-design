{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },

    "dev": {
      "cache": false,
      "persistent": true
    },

    "lint": {
      "outputs": []
    },

    "check-types": {
      "outputs": []
    },

    "clean": {
      "cache": false
    }
  }
}
